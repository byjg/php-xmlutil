{
  "name": "byjg/xmlutil",
  "description": "A utility class to make easy work with XML in PHP.",
  "autoload": {
    "psr-4": {
      "ByJG\\XmlUtil\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1 <8.4",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-simplexml": "*",
    "byjg/serializer": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "vimeo/psalm": "^5.20"
  },
  "license": "MIT"
}
